{
  "name": "fast-requery",
  "version": "1.1.4",
  "description": "Chrome extension for URL parameter management",
  "main": "src/popup.js",
  "scripts": {
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "test:watch": "npx jest --config .jest.config.js --watch",
    "build": "bash scripts/build.sh",
    "build:clean": "npx rimraf dist build && npm run build",
    "build:prod": "NODE_ENV=production npm run build:clean",
    "dev": "webpack --config webpack.config.js --watch",
    "validate": "node scripts/validate-build.js",
    "lint": "npx eslint . --ext .js",
    "lint:fix": "npx eslint . --ext .js --fix",
    "clean": "npx rimraf dist",
    "release": "bash scripts/release.sh",
    "cleanup": "bash scripts/cleanup.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "mini-css-extract-plugin": "^2.7.7",
    "rimraf": "^5.0.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "eslint-plugin-jest": "^27.9.0",
    "uglify-js": "^3.17.4",
    "clean-css-cli": "^5.6.2"
  }
}

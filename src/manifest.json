{
  "manifest_version": 2,
  "name": "URL Parameters Manager",
  "version": ".1.3",
  "description": "A Chrome extension for managing URL parameters",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en"
}
